        "sample-failed-test": "browserstack-node-sdk jest src/sample-failed.test.js",
        "sample-android-test": "browserstack-node-sdk jest src/sample-android.test.js",
        "sample-and-failed-test": "browserstack-node-sdk jest src/sample.test.js src/sample-failed.test.js",
